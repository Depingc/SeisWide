\par 
\par /* segy.h - include file for SEGY traces
\par  *
\par  * declarations for:
\par  *\tab typedef struct \{\} segy - the trace identification header
\par  *\tab typedef struct \{\} bhed - binary header
\par  *
\par  * Reference:
\par  *\tab K. M. Barry, D. A. Cavers and C. W. Kneale, "Special Report:\tab Recommended Standards for Digital Tape Formats", Geophysics, vol. 40, no. 2 (April 1975), P. 344-352.
\par  */ 
\par 
\par #ifndef SEGY_H
\par #define SEGY_H
\par #define SU_NFLTS\tab 60000\tab /* Arbitrary limit on data array size\tab */
\par /* TYPEDEFS */
\par typedef struct \{\tab /* segy - trace identification header */
\par \tab int tracl;\tab /* trace sequence number within line */
\par \tab int tracr;\tab /* trace sequence number within reel */
\par \tab int fldr;\tab \tab /* field record number */
\par \tab int tracf;\tab /* trace number within field record */
\par \tab int ep;\tab \tab /* energy source point number */
\par \tab int cdp;\tab /* CDP ensemble number */
\par \tab int cdpt;\tab /* trace number within CDP ensemble */
\par \tab short trid;\tab /* trace identification code: i2buf[14]
\par \tab \tab \tab 1 = seismic data
\par \tab \tab \tab 2 = dead
\par \tab \tab \tab 3 = dummy
\par \tab \tab \tab 4 = time break
\par \tab \tab \tab 5 = uphole
\par \tab \tab \tab 6 = sweep
\par \tab \tab \tab 7 = timing
\par \tab \tab \tab 8 = water break
\par \tab \tab \tab 9---, N = optional use (N = 32,767)
\par 
\par \tab \tab \tab Following are CWP id flags:
\par \tab \tab \tab  9 = autocorrelation
\par \tab \tab \tab 10 = Fourier transformed - no packing xr[0],xi[0], ..., xr[N-1],xi[N-1]
\par \tab \tab \tab 11 = Fourier transformed - unpacked Nyquist xr[0], xi[0],...,xr[N/2], xi[N/2]
\par \tab \tab \tab 12 = Fourier transformed - packed Nyquist even N:
\par \tab \tab \tab      xr[0],xr[N/2],xr[1],xi[1], ...,xr[N/2 -1],xi[N/2 -1] (note the exceptional second entry) odd N: xr[0],xr[(N-1)/2],xr[1],xi[1], ..., xr[(N-1)/2 -1],xi[(N-1)/2 -1],xi[(N-1)/2]\tab (note the exceptional second & last entries)
\par \tab \tab \tab 13 = Complex signal in the time domain xr[0],xi[0], ..., xr[N-1],xi[N-1]
\par \tab \tab \tab 14 = Fourier transformed - amplitude/phase a[0],p[0], ..., a[N-1],p[N-1]
\par \tab \tab \tab 15 = Complex time signal - amplitude/phase a[0],p[0], ..., a[N-1],p[N-1]
\par \tab \tab \tab 16 = Real part of complex trace from 0 to Nyquist
\par \tab \tab \tab 17 = Imag part of complex trace from 0 to Nyquist
\par \tab \tab \tab 18 = Amplitude of complex trace from 0 to Nyquist
\par \tab \tab \tab 19 = Phase of complex trace from 0 to Nyquist
\par \tab \tab \tab 21 = Wavenumber time domain (k-t)
\par \tab \tab \tab 22 = Wavenumber frequency (k-omega)
\par \tab \tab \tab 23 = Envelope of the complex time trace
\par \tab \tab \tab 24 = Phase of the complex time trace
\par \tab \tab \tab 25 = Frequency of the complex time trace
\par \tab \tab \tab 30 = Depth-Range (z-x) traces
\par \tab \tab \tab 101 = Seismic data packed to bytes (by supack1)
\par \tab \tab \tab 102 = Seismic data packed to 2 bytes (by supack2)
\par \tab \tab \tab */
\par \tab short nvs;\tab /* number of vertically summed traces (see vscode in bhed structure) */
\par \tab short nhs;\tab /* number of horizontally summed traces (see vscode in bhed structure) */
\par \tab short duse;\tab /* data use: 1 = production;  2 = test */
\par \tab int offset;\tab /* distance from source point to receiver i2buf[18-19] group (negative if opposite to direction in which the line was shot) */
\par \tab int gelev;\tab /* receiver group elevation from sea level (above sea level is positive) */
\par \tab int selev;\tab /* source elevation from sea level (above sea level is positive) */
\par \tab int sdepth;\tab /* source depth (positive) */
\par \tab int gdel;\tab /* datum elevation at receiver group */
\par \tab int sdel;\tab /* datum elevation at source */
\par \tab int swdep;\tab /* water depth at source */
\par \tab int gwdep;\tab /* water depth at receiver group */
\par \tab short scalel;\tab /* scale factor for previous 7 entries with value plus or minus 10 to the power 0, 1, 2, 3, or 4 (if positive, multiply, if negative divide) */
\par \tab short scalco;\tab /* scale factor for next 4 entries with value plus or minus 10 to the power 0, 1, 2, 3, or 4 (if positive, multiply, if negative divide) */
\par \tab int  sx;\tab /* X source coordinate  i2buf[36-37] */
\par \tab int  sy;\tab /* Y source coordinate */
\par \tab int  gx;\tab /* X group coordinate */
\par \tab int  gy;\tab /* Y group coordinate   i2buf[42-43] */
\par \tab short counit;\tab /* coordinate units code: for previous four entries
\par \tab \tab \tab \tab 1 = length (meters or feet)
\par \tab \tab \tab \tab 2 = seconds of arc (in this case, the X values are longitude and the Y values are latitude, a positive value designates the number of seconds east of Greenwich or north of the equator */
\par \tab short wevel;\tab /* weathering velocity */
\par \tab short swevel;\tab /* subweathering velocity */
\par \tab short sut;\tab /* uphole time at source */
\par \tab short gut;\tab /* uphole time at receiver group */
\par \tab short sstat;\tab /* source static correction */
\par \tab short gstat;\tab /* group static correction i2buf[50]*/
\par \tab short tstat;\tab /* total static applied */
\par \tab short laga;\tab /* lag time A, time in ms between end of 240-byte trace ident
ification header and time break, positive if time break occurs after end of header, time break is defined as the initiation pulse which maybe recorded on an auxiliary trace or as otherwise specified by the recording system */
\par \tab short lagb;\tab /* lag time B, time in ms between the time break  and the initiation time of the energy source, may be positive or negative i2buf[53]*/
\par \tab short delrt;\tab /* delay recording time, time in ms between initiation time of energy source and time when recording of data samples begins (for deep water work if recording does not start at zero time) i2buf[54]*/
\par \tab short muts;\tab /* mute time--start */
\par \tab short mute;\tab /* mute time--end */
\par \tab unsigned short ns;\tab /* number of samples in this trace */
\par \tab unsigned short dt;\tab /* sample interval; in micro-seconds */
\par \tab short gain;\tab /* gain type of field instruments code:
\par \tab \tab \tab \tab 1 = fixed
\par \tab \tab \tab \tab 2 = binary
\par \tab \tab \tab \tab 3 = floating point
\par \tab \tab \tab \tab 4 ---- N = optional use */
\par \tab short igc;\tab /* instrument gain constant */
\par \tab short igi;\tab /* instrument early or initial gain */
\par \tab short corr;\tab /* correlated:1 = no; 2 = yes */
\par \tab short sfs;\tab /* sweep frequency at start */
\par \tab short sfe;\tab /* sweep frequency at end i2buf[64]*/
\par \tab short slen;\tab /* sweep length in ms */
\par \tab short styp;\tab /* sweep type code:
\par \tab \tab \tab \tab 1 = linear
\par \tab \tab \tab \tab 2 = cos-squared
\par \tab \tab \tab \tab 3 = other */
\par \tab short stas;\tab /* sweep trace length at start in ms */
\par \tab short stae;\tab /* sweep trace length at end in ms */
\par \tab short tatyp;\tab /* taper type: 1=linear, 2=cos^2, 3=other */
\par \tab short afilf;\tab /* alias filter frequency if used */
\par \tab short afils;\tab /* alias filter slope */
\par \tab short nofilf;\tab /* notch filter frequency if used */
\par \tab short nofils;\tab /* notch filter slope */
\par \tab short lcf;\tab /* low cut frequency if used */
\par \tab short hcf;\tab /* high cut frequncy if used */
\par \tab short lcs;\tab /* low cut slope */
\par \tab short hcs;\tab /* high cut slope */
\par \tab short year;\tab /* year data recorded */
\par \tab short day;\tab /* day of year */
\par \tab short hour;\tab /* hour of day (24 hour clock) i2buf[80]*/
\par \tab short minute;\tab /* minute of hour */
\par \tab short sec;\tab /* second of minute */
\par \tab short timbas;\tab /* time basis code:
\par \tab \tab \tab \tab 1 = local
\par \tab \tab \tab \tab 2 = GMT
\par \tab \tab \tab \tab 3 = other */
\par \tab short trwf;\tab /* trace weighting factor, defined as 1/2^N volts for the least sigificant bit */
\par \tab short grnors;\tab /* geophone group number of roll switch position one */
\par \tab short grnofr;\tab /* geophone group number of trace one within original field record */
\par \tab short grnlof;\tab /* geophone group number of last trace within original field record */
\par \tab short gaps;\tab /* gap size (total number of groups dropped) */
\par \tab short otrav;\tab /* overtravel taper code:
\par \tab \tab \tab \tab 1 = down (or behind)
\par \tab \tab \tab \tab 2 = up (or ahead) i2buf[89]*/
\par \tab /* local assignments */
\par \tab float d1;\tab /* sample spacing for non-seismic data */
\par \tab float f1;\tab /* first sample location for non-seismic data */
\par \tab float d2;\tab /* sample spacing between traces */
\par \tab float f2;\tab /* first trace location */
\par \tab float ungpow;\tab /* negative of power used for dynamic range compression */
\par \tab float unscale;\tab /* reciprocal of scaling factor to normalize range */
\par \tab int ntr; \tab /* number of traces */
\par \tab short mark;\tab /* mark selected traces i2buf[104]*/
\par \tab long t1;  /* trace delay time in micro sec, to be added to delrt, segy extension by Deping! i2buf[105-106]*/
\par \tab short unass[13];\tab /* unassigned--NOTE: last entry causes a break in the word alignment, if we REALLY  want to maintain 240 bytes, the following entry should be an odd number of short/UINT2 OR do the insertion above the "mark" keyword entry */

\par \tab float  data[SU_NFLTS];
\par \} segy;
\par 
\par --------------------------------end of c struct.
\par }\pard\plain \ql \li0\ri0\nowidctlpar\faauto\rin0\lin0\itap0 \fs24\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {
